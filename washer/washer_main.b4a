				sb_url.Append("http://www.k91d.com/service/index.php?head=0527&type=ter&data=-1:-1:-1:-1:-1:-1:-1:-1")
			End If
			
			wviewEx.addJavascriptInterface(wv_pay_Ser,"B4A")
